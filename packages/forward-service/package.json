{
  "name": "forward-service",
  "packageManager": "yarn@3.1.1",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "concurrently \"tsc --watch\" \"nodemon --watch dist -e js --quiet dist/index.js\"",
    "deploy": "./deploy.sh",
    "print-config": "gcloud secrets versions access latest --secret=ciccd-config | tee ./config.yaml"
  },
  "dependencies": {
    "@google-cloud/cloudbuild": "^2.6.0",
    "@google-cloud/pubsub": "^2.18.4",
    "express": "^4.18.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.30",
    "concurrently": "^7.1.0",
    "dotenv": "^16.0.0",
    "nodemon": "^2.0.16",
    "typescript": "^4.5.4"
  }
}
